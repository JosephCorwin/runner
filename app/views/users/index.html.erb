<% provide(:title, "User Index") %>
<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.email, user %>
      <%= link_to "Delete User", user, method: 'delete', data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>

</ul>

<%= will_paginate %>
